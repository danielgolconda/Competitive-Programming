#include <iostream>
#include <bits/stdc++.h>
#include <algorithm>
#define lld long long int 
#define mod 1000000007
#define FIO ios::sync_with_stdio(false);cin.tie(0);cout.tie(0);
using namespace std;
int main() 
{
    FIO
    int tc;
    cin >> tc;
    while(tc--)
    {
        int n;
        cin >> n;
        double ans=0;
        for(int i=1;i<=n;i++)
        {
            ans+=(n/(i*1.0));
        }
        cout<<fixed<<setprecision(8)<<ans<<endl;
    }
	return 0;
}
